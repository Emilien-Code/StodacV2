nt-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;\r\n  background-color: #ffffff;\r\n  top: 170px;\r\n  right : 280px;\r\n  position: absolute;\r\n  width: 200px;\r\n  box-shadow: rgba(0, 0, 0, 0.24) 0 3px 8px;\r\n  cursor: pointer;\r\n  border-radius: 10px;\r\n  transition: height 0.5s ease-out;\r\n}\r\nli{\r\n  padding: 10px;\r\n  border-radius: 10px;\r\n  list-style: none;\r\n}\r\nbutton:hover, li:hover{\r\n  background-color: rgba(0, 0, 0, 0.24);\r\n}\r\n.selected{\r\n  background-color: rgba(0, 0, 0, 0.24);\r\n}\r\n</style>"],"sourceRoot":""}]}