: function () {\r\n      this.loaded = true;\r\n      window.paypal\r\n          .Buttons({\r\n            createOrder: (data, actions) => {\r\n              return actions.order.create({\r\n                purchase_units: [\r\n                  {\r\n                    description: this.description,\r\n                    amount: {\r\n                      value: this.userInfos.savePrix.prix_ttl\r\n                    }\r\n                  }\r\n                ]\r\n              });\r\n            },\r\n            onApprove: async (data, actions) => {\r\n              const order = await actions.order.capture();\r\n              this.data;\r\n              this.paidFor = true;\r\n              this.sc()\r\n              this.saveFacture(order.id)\r\n            },\r\n            onError: err => {\r\n              console.log(err);\r\n            }\r\n          })\r\n          .render(this.$refs.paypal);\r\n    },\r\n  },\r\n  computed: {\r\n    total: function () {\r\n      let total = 0;\r\n      for(let i = 0; i<this.$store.state.pannier.length; i++){\r\n        total += this.$store.state.pannier[i].article.price * this.$store.state.pannier[i].qty;\r\n      }\r\n      return total\r\n    },\r\n    ...mapState(['userInfos']),\r\n  },\r\n  props:{\r\n    saveFacture:Function,\r\n    sc:Function\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n#paypal-button-container{\r\n  display: flex;\r\n  justify-content: center;\r\n  z-index: 10;\r\n  background-color: #fff;\r\n}\r\n</style>"],"sourceRoot":""}]}