oduit\">\n                        <img :src=\"result.img\" alt=\"\">\n                        <div class=\"txts\">\n                            <span class=\"product-name\">{{result.name}}</span><br>\n                            <span class=\"product-category\">{{result.category}}</span><br>\n                            <span class=\"product-price\">{{result.price}}</span>\n                        </div>\n\n                    </div>\n                    \n                </li>\n\n            \n            </ul>\n            \n            <button v-if=\"resultArray[0]\" @click=\"search()\">Afficher tous les résultats</button>\n            <div id=\"noResult\" v-if=\"word && !resultArray[0]\"> Aucuns résultats </div>\n\n        </div> \n\n    </div>\n\n  \n</template>\n\n<script>\nconst axios = require(\"axios\") ;\nexport default {\n    name : 'search',\n    data: function(){\n        return {\n            word: '',\n            isSearch : false,\n            resultArray: []\n        }\n    },\n    methods:{\n        search : function () {\n            if(this.word){\n                this.$store.dispatch('getStuffByFirstLetters', this.word)\n                .then(this.isSearch = false)\n                .catch(\"ERREUR REQUEST\")\n            }else{\n                this.$store.dispatch('getStufs', 1)\n                .then(this.isSearch = false)\n                .catch(\"ERREUR REQUEST\")\n                this.$store.dispatch('getStuffNb')\n                .then(this.isSearch = false)\n                .catch(\"ERREUR REQUEST\")\n            }\n            },\n        clicked: function(){\n            this.isSearch = true;\n        },\n        results : function(){\n            if(this.word){\n                axios.get(`http://localhost:3000/api/stuff/name/${this.word}/4`)\n                .then((response)=>{\n                    this.resultArray =  response.data\n                    console.log(this.resultArray)\n                })\n                .catch((error)=>{\n                    console.log(error)\n                })\n            }\n        },\n        close: function (){\n            this.isSearch = false\n        },\n        pushToID : function(item){\n            this.$router.push(`/produit/${item._id}`);\n        },\n    } \n\n}\n</script>\n\n<style scoped>\n.product-category{\n  font-size: .8em;\n}\n.product-name{\n  font-size: 1.2em;\n}\n.product-price{\n  color: #007057;\n  font-weight : bold;\n}               \n* {\n  font-family: 'Segoe UI', sans-serif;\n}\n#noResult{\n    padding: 10px;\n}\n.Produit{\n    display: flex;\n    flex-direction:row;\n    margin: 15px 0 ;\n    margin-left: 10px;\n}\nimg{\n    width: 100px;\n    margin-right: 10px;\n}\n#search {\n    box-shadow: rgba(0, 0, 0, 0.24) 0px 3px 8px;\n    background-color: #ffffff;\n    border-radius: 10px;\n    width: 400px;\n    position: absolute;\n    top: 185px;\n    left : 60px;\n}\n\ninput {\n    padding: 10px;\n    width: 400px;\n    border-radius: 10px;\n    font-size: 1em;\n    border: none;\n    border-bottom: solid  1px #C3C3C3;\n }\nbutton{\n    position: relative;\n    left: 50%;\n    transform: translateX(-50%);\n    font-size: 1em;\n    padding: 10px;\n    border-radius: 10px;\n    border: none;\n    cursor: pointer;\n    margin: 10px 0;\n}\nbutton:hover{\n    background-color: #C3C3C3;\n}\nul{\n    list-style: none;\n}\nli{\n     cursor:pointer;\n     border-radius: 10px;\n    transition: .5s;\n}\nli:hover{\n    background-color: #e3e3e3;\n    transition: .5s;\n}\n</style>"],"sourceRoot":""}]}