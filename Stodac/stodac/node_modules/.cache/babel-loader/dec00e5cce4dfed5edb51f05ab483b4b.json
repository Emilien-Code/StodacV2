dispatch('getStuffNb')\r\n        .then(()=>{console.log('nbStuff chargé')})\r\n        .catch(()=>{console.log('Impossible de charger le nombre Stuffs')})\r\n  },\r\n  methods:{\r\n    loadMore : function(nb){\r\n      this.nbLoadMore++;\r\n      this.$store.dispatch('getStufs', nb)\r\n          .then(()=>{console.log('Stuffs chargés')})\r\n          .catch(()=>{console.log('Impossible de charger les Stuffs')})\r\n    }\r\n  },\r\n  components:{\r\n    test : produitAdminLite\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n#buttons_wrapper{\r\n  width: 100vw;\r\n  display: flex;\r\n  align-items: center ;\r\n  justify-content: center;\r\n}\r\n#product{\r\n  margin-top: 70px;\r\n}\r\nbutton{\r\n  width: 50px;\r\n  height: 50px;\r\n  margin-left: 10px;\r\n  border-radius: 10px;\r\n  border: none;\r\n  box-shadow: rgba(0, 0, 0, 0.24) 0 3px 8px;\r\n  background-color:#ffffff ;\r\n  cursor: pointer;\r\n  transition: .5s;\r\n}\r\nbutton:hover{\r\n  background: #e3e3e3;\r\n  transition: .5s;\r\n}\r\n</style>"],"sourceRoot":""}]}