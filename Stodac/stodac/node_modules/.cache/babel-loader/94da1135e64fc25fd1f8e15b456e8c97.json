M,CAjBR,CAkBQ,CApBV,CA6BI,EARJ,mBAAA,CAOU,SAPV,EAAA,WAAA,EAOU,CALR,WAKQ,EAHK,IAAA,CAAA,OAAA,CAAQ,Y,kBAArB,mBAAA,CAEQ,OAFR,EAEQ,WAFR,EAEQ,E,kBADN,mBAAA,CAA0E,SAA1E,EAA0E,IAA1E,EAA0E,WAAA,CAAvD,IAAA,CAAA,OAAA,CAAQ,YAA+C,EAAnC,UAA5B,IAA4B,EAAxB;yBAAf,mBAAA,CAA0E,IAA1E,EAA0E;AAAhC,MAAA,GAAG,EAAE;AAA2B,KAA1E,EAAmD,CAAG,mBAAA,CAAe,GAAf,EAAe,IAAf,EAAe,gBAAA,CAAV,IAAU,CAAf,EAAS;AAAA;AAAT,KAAH,CAAnD,C;GAA0E,CAA1E,E;;AAAA,GACM,EAFR,C,qCAGQ,CAPV,CAQI,CA3DN,C","sourcesContent":["<template>\r\n  <div class=\"Produit\">\r\n        <h1>{{ article.name }}</h1>\r\n    <section id=\"Un\">\r\n      <img :src=\"article.img\" alt=\"Image d'illustration\">\r\n      <div class=\"PriceAndInfos\">\r\n        <p class=\"title\">Description</p>\r\n        <p>{{article.description}}</p>\r\n        <p><span class=\"title\">Prix : </span>{{article.price}}€</p>\r\n        <div class=\"container\">\r\n          <input type=\"number\" v-model=\"qty\" id=\"qty\" min=\"1\" :max=\"article.qty\"/>\r\n          <div class=\"more_less\">\r\n            <button class=\"qty_button\" id=plus @click=\"more()\">\r\n              <div id=\"plus_vertical\"></div>\r\n              <div id=\"plus_horizontal\"></div>\r\n              </button>\r\n            <button class=\"qty_button\" @click=\"less()\">\r\n              <div id=\"plus_horizontal\"></div>\r\n            </button>\r\n          </div>\r\n          <button class=\"button\" @click=\"addToPannier()\">Ajouter au pannier</button>\r\n        </div>\r\n            <p v-if=\"this.qty > this.article.qty\">{{article.qty}} article(s) en stock</p>\r\n      </div>\r\n    </section>\r\n    <section id=\"deux\">\r\n      <p><span class=\"title\">Modèles compatibles</span></p>\r\n      <table>\r\n        <tr v-for=\"test in article.compatibility\" :key=\"test\" ><p>{{test}}</p></tr>\r\n      </table>\r\n    </section>\r\n    <section id=\"trois\">\r\n      <p><span class=\"title\">Fiche technique</span></p>\r\n      <table style=\"font-size:1.4em\">\r\n        <tr>\r\n          <td>Article</td>\r\n          <td>{{article.name}}</td>\r\n        </tr>\r\n        <tr>\r\n          <td>Fabriquant</td>\r\n          <td>{{article.manufacturer}}</td>\r\n        </tr>\r\n        <tr>\r\n          <td>Référence</td>\r\n          <td>{{article.reference}}</td>\r\n        </tr>\r\n        <tr>\r\n          <td>État du Produit</td>\r\n          <td>{{article.state}}</td>\r\n        </tr>\r\n      </table>\r\n    </section>\r\n    <section id=\"quatre\">\r\n\r\n      <p><span class=\"title\">Commentaires</span></p>\r\n\r\n      <table v-if=\"article.commentaires\">\r\n        <tr v-for=\"test in article.commentaires\" :key=\"test\" ><p>{{test}}</p></tr>\r\n      </table>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n// @ is an alias to /src\r\nconst axios = require('axios')\r\n\r\nexport default {\r\n  name: 'Produit',\r\n  data: function(){ \r\n    return {\r\n      articleId: this.$route.params.id,\r\n      article: {},\r\n      qty : 1\r\n    }\r\n  },\r\n  methods:{\r\n    more : function(){\r\n      if(this.qty<this.article.qty){\r\n        this.qty+=1;\r\n      }\r\n    },\r\n    less : function(){\r\n      if(this.qty>1){\r\n        this.qty--;\r\n      }\r\n    },\r\n    addToPannier : function(){\r\n      const a = {'article' : this.article,'qty' : this.qty};\r\n\r\n      this.$store.dispatch('addPannier', a)\r\n    }\r\n  },\r\n  mounted(){\r\n      axios.get(`http://localhost:3000/api/stuff/product/${this.articleId}`)\r\n      .then((response)=>{\r\n        this.article =  response.data[0]\r\n      })\r\n      .catch((error)=>{\r\n        console.log(error)\r\n      })\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n@import url('http://fonts.cdnfonts.com/css/segoe-ui-4');\r\n@import url('https://fonts.googleapis.com/css2?family=Poppins:wght@400;500;800&display=swap');\r\n#quatre{\r\n  padding-left: 10vw;\r\n}\r\n#trois{\r\n  padding: 10vw;\r\n}\r\n#deux{\r\n  margin-top: 150px;\r\n  padding: 10vw;\r\n  background-color: #EFEFEF;\r\n}\r\ntable{\r\n  width: 80vw;\r\n  min-width: 50%;\r\n}\r\n\r\n#qty  {\r\n  font-family: 'Segoe UI', sans-serif;\r\n  width: 55px;\r\n  height: 55px;\r\n  border: solid #419D79 2px;\r\n  border-radius: 17.5px;\r\n  margin-right: 10px;\r\n  font-size: 1.5em;\r\n  text-align: center;\r\n}\r\ninput::-webkit-outer-spin-button,\r\ninput::-webkit-inner-spin-button {\r\n  -webkit-appearance: none;\r\n  margin: 0;\r\n}\r\n/* Firefox */\r\ninput[type=number] {\r\n  -moz-appearance: textfield;\r\n}\r\n.container{\r\n  display: flex;\r\n}\r\n#plus_vertical{\r\n  height: 17px;\r\n  width: 2.5px;\r\n  background-color: #FFFFFF;\r\n  border-radius: 10px;\r\n  position: absolute;\r\n}\r\n#plus_horizontal{\r\n  height: 2.5px;\r\n  width: 17px;\r\n  background-color: #FFFFFF;\r\n  border-radius: 5px;\r\n}\r\n.more_less{\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: space-between;\r\n  height: 55px;\r\n  width: 25px;\r\n  margin-right: 30px;\r\n}\r\n.qty_button{\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  width: 25px;\r\n  height: 25px;\r\n  background-color: #419D78;\r\n  border: none;\r\n  border-radius: 5px;\r\n  transition: .4s background-color;\r\n}\r\n.title{\r\n  font-weight: bold;\r\n  color: #007057;\r\n}\r\np{\r\n  margin-bottom : 30px;\r\n  font-size: 1.4em;\r\n}\r\nh1{\r\n  color: #007057;\r\n  margin: 20px 0 20px 10vw;\r\n}\r\n*{\r\n  font-family: 'Segoe UI', sans-serif;\r\n  margin: 0;\r\n  padding: 0;\r\n  box-sizing: border-box;\r\n}\r\nimg{\r\n  width: 40vw;\r\n  max-height: 385px;\r\n  object-fit: cover;\r\n}\r\n.PriceAndInfos{\r\n  padding-left: 50px;\r\n  width: 40vw;\r\n}\r\nsection#Un{\r\n  display: flex;\r\n  justify-content: center;\r\n}\r\n.button {\r\n  font-family: 'poppins', sans-serif;\r\n  background: #419D79;\r\n  color:white;\r\n  border-radius: 8px;\r\n  font-weight: 800;\r\n  font-size: 15px;\r\n  border: none;\r\n  width: 250px;\r\n  padding: 16px;\r\n  transition: .4s background-color;\r\n}\r\nbutton:hover {\r\n  cursor:pointer;\r\n  background: #078A6C;\r\n}\r\n</style>"],"sourceRoot":""}]}